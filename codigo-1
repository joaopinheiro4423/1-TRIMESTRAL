const int led1 = 10;
const int led2 = 9;
const int led3 = 8;

int var = 0;

 void setup()
{
 pinMode(led1, OUTPUT);
 pinMode(led2, OUTPUT);
 pinMode(led3, OUTPUT);
  
 Serial.begin(9600);
}
 void loop()
{  
  digitalWrite(led1, HIGH);
  delay(25000);
  digitalWrite(led1, LOW);
  delay(1000);
  digitalWrite(led2, HIGH);
  delay(3000);
  digitalWrite(led2, LOW);
  delay(1000);
  digitalWrite(led3, HIGH);
  delay(30000);
  digitalWrite(led3, LOW);
  delay(1000);  
  }
